<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Anamenu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/Duyurular"
        android:layout_width="match_parent"
        android:layout_height="126dp"
        android:adjustViewBounds="true"
        android:foregroundGravity="center"
        android:gravity="center"
        android:padding="20dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/menud2" />

    <TextView
        android:id="@+id/Duyurularyazi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/Duyurular"
        android:layout_alignTop="@id/Duyurular"
        android:layout_alignRight="@id/Duyurular"
        android:layout_alignBottom="@id/Duyurular"
        android:layout_marginLeft="125dp"
        android:gravity="bottom"
        android:text="Duyurular"
        android:textSize="24sp" />

    <ImageButton
        android:id="@+id/Haberler"
        android:layout_width="183dp"
        android:layout_height="133dp"
        android:layout_marginTop="125dp"
        android:adjustViewBounds="true"
        android:foregroundGravity="center"
        android:gravity="center"
        android:padding="20dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/kulupler" />

    <TextView
        android:id="@+id/Haberleryazi"
        android:layout_width="153dp"
        android:layout_height="193dp"
        android:layout_alignLeft="@id/Haberler"
        android:layout_alignTop="@id/Haberler"
        android:layout_alignBottom="@id/Haberler"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="4dp"
        android:gravity="bottom"
        android:text="Blog"
        android:textSize="24sp" />

    <ImageButton
        android:id="@+id/Kollar"
        android:layout_width="wrap_content"
        android:layout_height="134dp"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="125dp"
        android:adjustViewBounds="true"
        android:foregroundGravity="center"
        android:gravity="center"
        android:padding="20dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/baskanlar" />

    <TextView
        android:id="@+id/Kollaryazi"
        android:layout_width="153dp"
        android:layout_height="193dp"
        android:layout_alignLeft="@id/Kollar"
        android:layout_alignTop="@id/Kollar"
        android:layout_alignBottom="@id/Kollar"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="5dp"
        android:gravity="bottom"
        android:text="Komiteler"
        android:textSize="24sp" />

    <ImageButton
        android:id="@+id/takvim"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="255dp"
        android:adjustViewBounds="true"
        android:foregroundGravity="center"
        android:gravity="center"
        android:padding="20dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/takvim" />

    <TextView
        android:id="@+id/Takvimyazi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/takvim"
        android:layout_alignTop="@id/takvim"
        android:layout_alignRight="@id/takvim"
        android:layout_alignBottom="@id/takvim"
        android:layout_marginLeft="100dp"
        android:gravity="bottom"
        android:text="Etkinlik Takvimi"
        android:textSize="24sp" />
</RelativeLayout>